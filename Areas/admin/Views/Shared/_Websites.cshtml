@model IEnumerable<SDES___Office_Directory.Models.website>

<h3>Websites</h3>
<table class="grid smaller">
    <tr>
        <th>
            <a href="@Url.Action("CreateFromOffice", "website", new { id = ViewBag.Office })">
                <img src="https://assets.sdes.ucf.edu/images/icons/fff_add.png" alt="icon" class="clean" />
            </a>
        </th>
        <th>
            Site
        </th>
        <th>
            Calendar
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "EditFromOffice", "website", new { id=item.websiteId }, null) |
            @Html.ActionLink("Delete", "DeleteFromOffice", "website", new { id = item.websiteId }, null)
        </td>
        <td>
            <a href="@item.websiteUri">@item.websiteName</a>
        </td>
        <td>
            <a href="http://events.ucf.edu/?calendar_id=@item.websiteCal&upcoming=upcoming">@item.websiteCal</a>
        </td>
    </tr>
}

</table>

